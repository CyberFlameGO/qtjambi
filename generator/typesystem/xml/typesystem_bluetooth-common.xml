  </value-type>
  <value-type name="QLowEnergyCharacteristicData">
        <modify-function signature="operator=(QLowEnergyCharacteristicData)" remove="all"/>
  </value-type>
  <value-type name="QLowEnergyConnectionParameters">
        <modify-function signature="operator=(QLowEnergyConnectionParameters)" remove="all"/>
  </value-type>
  <value-type name="QLowEnergyServiceData">
        <modify-function signature="operator=(QLowEnergyServiceData)" remove="all"/>
        <modify-function signature="setIncludedServices(const QList&lt;QLowEnergyService*>&amp;)">
            <inject-code position="end">
                if(__rcIncludedService!=null){
                    __rcIncludedService.clear();
                }else{
                    __rcIncludedService = new java.util.ArrayList&lt;>();
                }
                __rcIncludedService.addAll(services);
            </inject-code>
        </modify-function>
        <modify-function signature="addIncludedService(QLowEnergyService *)">
            <modify-argument index="1">
                <reference-count action="add" variable-name="__rcIncludedService"/>
            </modify-argument>
        </modify-function>
  </value-type>
  
  <object-type name="QBluetoothDeviceDiscoveryAgent">
  </object-type>
  <object-type name="QBluetoothLocalDevice">
  </object-type>
  <object-type name="QBluetoothSocket">
        <modify-function signature="errorString() const" rename="socketErrorString"/>
  </object-type>
  <object-type name="QBluetoothServer">
  </object-type>
  <object-type name="QBluetoothServiceDiscoveryAgent">
  </object-type>
  <object-type name="QBluetoothTransferManager">
  </object-type>
  <object-type name="QBluetoothTransferReply" until="5.*">
        <modify-function signature="setManager(QBluetoothTransferManager*)">
            <modify-argument index="1">
                <reference-count action="set" variable-name="__rcManager"/>
            </modify-argument>
        </modify-function>
  </object-type>
  <object-type name="QLowEnergyController">
        <modify-function signature="addService(QLowEnergyServiceData,QObject*)">
            <modify-argument index="2">
                <reference-count action="ignore"/>
            </modify-argument>
        </modify-function>
        <modify-function signature="createPeripheral(QBluetoothAddress,QObject*)" since="6.2.0">
            <modify-argument index="0">
                <define-ownership class="shell" owner="cplusplus"/>
                <define-ownership class="native" owner="java"/>
            </modify-argument>
        </modify-function>
  </object-type>
  <object-type name="QLowEnergyService">
  </object-type>
