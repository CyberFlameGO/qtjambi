<typesystem qt-library="QtTextToSpeech" description="Provides support for accessibility features such as text-to-speech.">
    <enum-type name="QTextToSpeech::State"/>
    <enum-type name="QTextToSpeech::BoundaryHint" since="6.4"/>
    <enum-type name="QTextToSpeech::ErrorReason" since="6.4"/>
    <object-type name="QTextToSpeech">
    </object-type>
    <object-type name="QTextToSpeechEngine">
    </object-type>
    <object-type name="QTextToSpeechPlugin">
    </object-type>
    <enum-type name="QVoice::Age"/>
    <enum-type name="QVoice::Gender"/>
    <value-type name="QVoice">
        <modify-function signature="operator=(const QVoice &amp;)" remove="all"/>
    </value-type>
</typesystem>
